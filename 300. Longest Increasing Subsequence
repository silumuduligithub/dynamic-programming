class Solution {
    public int lengthOfLIS(int[] nums) {
        int n = nums.length;
        int maxLen = 0;
        int[] dp = new int[n];
        for(int i = 0; i < n; i++){
            int max = 0;
            for(int j = i - 1; j >= 0; j--){
                if(nums[i] > nums[j]){
                    max = Math.max(dp[j], max);
                }
            }
            dp[i] = max + 1;
            maxLen = Math.max(dp[i], maxLen);
        }
        return maxLen;
    }
}
